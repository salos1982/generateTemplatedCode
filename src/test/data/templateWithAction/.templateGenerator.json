{
  "type": "workspace",
  "path": "template",
  "name": "test module with action",
  "variables": [
    {
      "name": "nameOfModule",
      "prompt": "Input name of module"
    }
  ],
  "actions": [
    {
      "type": "insertTemplateToFile",
      "fileName": "{{workspace_directory}}/modules/modules.list",
      "snippet": "// start list",
      "template": "{{nameOfModule}}.init();",
      "position": "after"
    }
  ]
}