{
  "name": "addmodule",
  "displayName": "addModule",
  "description": "Helps adding bunch of files for new module from template",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:salos.addModule"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "salos.addModule",
        "title": "Add new templated module"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "salos.addModule",
          "group": "2_workspace"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "unit test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "roots": [
      "<rootDir>/src/test/logic"
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "jest-extended": "^1.1.0",
    "mocha": "^9.1.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.3.1",
    "camelcase": "^6.2.0",
    "jest": "^27.3.1",
    "snake-case": "^3.0.4",
    "ts-node": "^10.3.0"
  }
}
